#!/usr/bin/env bash
# This script that gives you information about the holbertonschool file_name.

f_name="holbertonschool"
if [ -e "$f_name" ]
then
	echo "holbertonschool file exists"
	if [ -s "$f_name" ]
	then
		echo "holbertonschool file is not empty"
	else
		echo "holbertonschool file is empty"
	fi
	if [ -f "$f_name" ]
	then
		echo "holbertonschool is a regular f_name"
	fi
else
	echo "holbertonschool file does not exist"
fi
